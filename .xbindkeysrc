## launch demu
"dmenu_run -fn Hack-16:normal -l 5"
    m:0x40 + c:65
    Mod4 + space 

# launch browser
"lof /usr/lib/firefox/firefox"
    m:0x40 + c:45
    Mod4 + k

# launch or focus terminal
"lof st\ \-e\ tmux"
    m:0x40 + c:44
    Mod4 + j

# launch or focus editor
"lof_intellij"
    m:0x40 + c:43
    Mod4 + h

# launch or focus nemo
"lof nemo"
    m:0x40 + c:40
    Mod4 + d

# launch sound control
# lof not required as this is defualt behavior
"pavucontrol"
    m:0x40 + c:39
    Mod4 + s 

# resize window for OBS
"wmctrl -r :ACTIVE: -e 0,0,0,1520,1080"
    m:0x41 + c:10
    Shift+Mod4 + 1 

# resize window for sharing in zoom
"wmctrl -r :ACTIVE: -e 0,0,0,1920,1080"
    m:0x41 + c:11
    Shift+Mod4 + 2

# launch or focus outlook
"lof /opt/outlook/outlook"
    m:0x40 + c:29
    Mod4 + y 

# launch or focus notion
"lof /opt/notion/notion"
    m:0x40 + c:57
    Mod4 + n

# launch or focus slack
"lof /usr/lib/slack/slack"
    m:0x40 + c:30
    Mod4 + u
